{
  "name": "todolist",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "immutable": "^3.8.2",
    "node-sass": "^4.9.4",
    "npm-run-all": "^4.1.3",
    "parallel": "^1.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-scripts": "2.0.5",
    "react-transition-group": "^2.5.0",
    "redux": "^4.0.1",
    "redux-immutable": "^4.0.0",
    "sass-loader": "^7.1.0",
    "styled-components": "^4.0.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
