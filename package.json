  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "react-app-rewire-scss": "^1.0.2",
    "react-app-rewired": "^1.6.2",
    "webpack-cli": "^3.1.2"
  }
}
